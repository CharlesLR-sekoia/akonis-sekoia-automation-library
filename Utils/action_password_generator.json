{
    "uuid": "128b8b23-8535-4ee0-8510-6175573dbe6d",
    "name": "Password Generator",
    "docker_parameters": "password-generator",
    "description": "Generate a password securely based on specified parameters",
    "arguments": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "title": "Parameters",
        "type": "object",
        "properties": {
            "password_length": {
                "description": "Password length",
                "default": 16,
                "type": "integer"
            },
            "number_of_digits": {
                "description": "Number of digits in the password",
                "default": 2,
                "type": "integer"
            },
            "number_of_special_characters": {
                "description": "Number of special characters in the password",
                "default": 2,
                "type": "integer"
            }
        },
        "required": [
            "password_length",
            "number_of_digits",
            "number_of_special_characters"
        ]
    },
    "results": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object",
        "properties": {
            "password": {
                "type": "string"
            }
        }
    }
}
